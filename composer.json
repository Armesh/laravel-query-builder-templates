{
    "name": "chalcedonyt/laravel-query-builder-templates",
    "description": "Query Builder Templates for Laravel. Organizes complex queries that span multiple tables. Generates reusable scopes that can be applied into these templates.",
    "keywords": [
        "laravel",
        "query",
        "complex query",
        "report",
        "scope",
        "template",
        "builder"
    ],
    "homepage": "https://github.com/chalcedonyt/laravel-query-builder-templates",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Teoh",
            "email": "chalcedonyt@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "illuminate/database": "~5.1",
        "illuminate/filesystem": "~5.1",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Chalcedonyt\\QueryBuilderTemplate\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
